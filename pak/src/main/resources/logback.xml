<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%level %d{MM-dd-yyyy HH:mm:ss} %thread - %msg%n</pattern>
        </encoder>
    </appender>
         
    <root level="debug">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>